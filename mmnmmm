                elseif HealthMs == "20" then
                    MobHealthFruits = 20
                elseif HealthMs == "25" then
                    MobHealthFruits = 25
                elseif HealthMs == "30" then
                    MobHealthFruits = 30
                elseif HealthMs == "35" then
                    MobHealthFruits = 35
                elseif HealthMs == "40" then
