                elseif HealthMs == "20" then
                    MobHealthFruits = 20
                elseif HealthMs == "25" then
                    MobHealthFruits = 25
                elseif HealthMs == "30" then
                    MobHealthFruits = 30
                elseif HealthMs == "35" then
                    MobHealthFruits = 35
                elseif HealthMs == "40" then
                    MobHealthFruits = 40
                elseif HealthMs == "45" then
                    MobHealthFruits = 45
                elseif HealthMs == "50" then
                    MobHealthFruits = 50
                elseif HealthMs == "55" then
                    MobHealthFruits = 55
                elseif HealthMs == "60" then
                    MobHealthFruits = 60
                elseif HealthMs == "70" then
                    MobHealthFruits = 70
                elseif HealthMs == "80" then
